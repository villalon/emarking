				<FIELD NAME="overall_fairness" TYPE="int" LENGTH="10" NOTNULL="true"
					DEFAULT="0" SEQUENCE="false" />
				<FIELD NAME="expectation_reality" TYPE="int" LENGTH="10"
					NOTNULL="true" DEFAULT="0" SEQUENCE="false" />
